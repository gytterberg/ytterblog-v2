    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.16",
    "babel-loader": "^8.2.2",
    "chai-exclude": "^2.0.3",
    "css-loader": "^5.2.6",
    "deep-equal-in-any-order": "^1.1.7",
    "style-loader": "^2.0.0",
    "supertest": "^4.0.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.19.0",
    "chai": "^4.3.4",
    "chalk": "^2.4.2",
    "chokidar": "^3.5.1",
    "express": "^4.17.1",
    "mocha": "^8.4.0",
